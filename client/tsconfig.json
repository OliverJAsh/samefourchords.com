{
    "compilerOptions": {
        "noImplicitAny": true,
        "target": "es5",
        "outDir": "./target",
        "allowJs": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "noImplicitThis": true,
        "noImplicitReturns": true,
        "strictNullChecks": true
    },
    "exclude": ["./target"]
}
