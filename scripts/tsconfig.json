{
    "compilerOptions": {
        "noImplicitAny": true,
        "target": "es6",
        "module": "commonjs",
        "allowJs": true,
        "outDir": "./target",
        "sourceMap": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "noImplicitThis": true,
        "noImplicitReturns": true,
        "strictNullChecks": true
    },
    "include": [
        "./src"
    ]
}
